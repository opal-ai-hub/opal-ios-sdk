@objc @objcMembers public class OpalCustomEventItem : ObjectiveC.NSObject {
  @objc public var id: Swift.String
  @objc public var type: Swift.String
  @objc public var name: Swift.String?
  @objc public var data: [Swift.String : Any]?
  @objc public init(id: Swift.String, type: Swift.String, name: Swift.String? = nil, data: [Swift.String : Any]? = nil)
  @objc deinit
}
@objc @objcMembers final public class OpalRetailItem : OpalSDK.OpalCustomEventItem {
  @objc final public var brand: Swift.String?
  @objc final public var price: Foundation.Decimal
  @objc final public var currencyCode: Swift.String
  @objc public init(id: Swift.String, type: Swift.String, name: Swift.String? = nil, brand: Swift.String? = nil, price: Foundation.Decimal, currencyCode: Swift.String, data: [Swift.String : Any]? = nil)
  @objc deinit
}
