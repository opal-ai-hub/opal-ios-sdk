  @objc public static func completeCheckout(totalPrice: Foundation.Decimal, currency: Swift.String, itemCount: Swift.Int, data extra: [Swift.String : Any]? = nil) throws
  @objc override public init(id: Swift.String, type: Swift.String, name: Swift.String? = nil, data: [Swift.String : Any]? = nil)
  @objc deinit
}
